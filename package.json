{
    "name": "sveltekit-api-fetch",
    "version": "1.0.0",
    "author": "Laurent St√©phenne <laurent@guibi.ca>",
    "description": "Adding type safety to SvelteKit's server endpoints.",
    "keywords": [
        "sveltekit",
        "typescript",
        "api",
        "type-safety",
        "endpoint",
        "zod",
        "validation"
    ],
    "license": "MIT",
    "homepage": "https://www.npmjs.com/package/sveltekit-api-fetch",
    "bugs": "https://github.com/Guibi1/sveltekit-api-fetch/issues",
    "repository": {
        "url": "https://github.com/Guibi1/sveltekit-api-fetch"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/index.js",
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js",
            "import": "./dist/index.js"
        },
        "./vite": {
            "types": "./dist/vite/index.d.ts",
            "svelte": "./dist/vite/index.js",
            "import": "./dist/vite/index.js"
        }
    },
    "typesVersions": {
        ">4.0": {
            "index.d.ts": [
                "./dist/index.d.ts"
            ],
            "vite": [
                "./dist/vite/index.d.ts"
            ]
        }
    },
    "peerDependencies": {
        "@sveltejs/kit": "1.x",
        "svelte": "^3.55.0 || ^4.0.0-next.0 || ^4.0.0",
        "zod": "3.x"
    },
    "devDependencies": {
        "@sveltejs/kit": "^1.20.2",
        "@types/node": "^20.3.2",
        "acorn": "^8.9.0",
        "acorn-walk": "^8.2.0",
        "glob": "^10.3.0",
        "prettier": "^2.8.8",
        "typescript": "^5.1.3",
        "vite": "^4.3.9",
        "zod": "^3.21.4"
    },
    "type": "module",
    "packageManager": "pnpm@8.5.1+",
    "scripts": {
        "build": "tsc",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write ."
    }
}
