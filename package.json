{
    "name": "sveltekit-api-fetch",
    "version": "3.0.0",
    "author": "Laurent St√©phenne <laurent@guibi.ca>",
    "description": "Adding type safety to SvelteKit's server endpoints.",
    "keywords": [
        "sveltekit",
        "typescript",
        "api",
        "type-safety",
        "endpoint",
        "zod",
        "validation"
    ],
    "license": "MIT",
    "homepage": "https://www.npmjs.com/package/sveltekit-api-fetch",
    "bugs": "https://github.com/Guibi1/sveltekit-api-fetch/issues",
    "repository": {
        "url": "https://github.com/Guibi1/sveltekit-api-fetch"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "types": "dist/index.d.ts",
    "typesVersions": {
        "*": {
            "vite": [
                "dist/vite/index.d.ts"
            ]
        }
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./vite": {
            "types": "./dist/vite/index.d.ts",
            "import": "./dist/vite/index.js"
        },
        "./package.json": "./package.json"
    },
    "peerDependencies": {
        "@sveltejs/kit": "^1.22.0",
        "svelte": "^3.55.0 || ^4.0.0-next.0 || ^4.0.0",
        "zod": "3.x"
    },
    "devDependencies": {
        "@types/node": "^20.3.3",
        "prettier": "^3.0.0",
        "prettier-plugin-organize-imports": "^3.2.2",
        "vite": "^4.3.9"
    },
    "dependencies": {
        "glob": "^10.3.1",
        "typescript": "^5.1.6",
        "zod": "^3.21.4"
    },
    "type": "module",
    "packageManager": "pnpm@8.5.1+",
    "scripts": {
        "build": "tsc",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write ."
    }
}
